    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define",
                "DART_DEFINE_APP_ENV=debug"
            ]
        },
        {
            "name": "Run Test",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "DART_DEFINE_APP_ENV=test"
            ]
        },
        {
            "name": "Run Prod",
            "request": "launch",
            "type": "dart", 
            "args": [
                "--dart-define",
                "DART_DEFINE_APP_ENV=prod"
            ]
        }
    ]
}